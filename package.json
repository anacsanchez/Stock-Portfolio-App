{
  "name": "stock-portfolio",
  "version": "1.0.0",
  "description": "web-based stock portfolio app",
  "main": "server/index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:client-watch": "webpack-dev-server --config webpack.dev.js",
    "build:watch": "webpack -w --config webpack.dev.js",
    "start": "node server/index.js",
    "start:dev": "npm-run-all build:watch start:dev-server",
    "start:dev-client": "npm run build:client-watch",
    "start:dev-server": "cross-env NODE_ENV=\"development\" nodemon server/index.js",
    "storybook": "start-storybook -p 3456",
    "test": "cross-env NODE_ENV=\"test\" mocha test/*.spec.js --watch --recursive"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "public/*"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "3.0.0",
    "@babel/runtime": "^7.8.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@reach/router": "^1.2.1",
    "apollo": "^2.16.3",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-datasource-rest": "^0.7.0",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.15",
    "apollo-server": "^2.10.1",
    "bcrypt": "^3.0.8",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "sequelize": "^5.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-a11y": "^5.3.9",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-backgrounds": "^5.3.9",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-cssresources": "^5.3.9",
    "@storybook/addon-docs": "^5.3.9",
    "@storybook/addon-events": "^5.3.9",
    "@storybook/addon-info": "^5.3.9",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addon-notes": "^5.3.9",
    "@storybook/addon-storysource": "^5.3.9",
    "@storybook/addon-viewport": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "apollo-storybook-react": "^0.2.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-graphql-tag": "^2.5.0",
    "cross-env": "^7.0.0",
